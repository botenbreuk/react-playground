{
  "name": "react-playground",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@42.nl/jarb-final-form": "2.0.3",
    "@42.nl/react-error-store": "1.1.2",
    "@42.nl/spring-connect": "^4.3.0",
    "bootstrap": "5.1.0",
    "classnames": "2.3.1",
    "final-form": "4.20.2",
    "immutability-helper": "3.1.1",
    "lodash": "4.17.21",
    "moment": "^2.29.1",
    "react": "17.0.2",
    "react-dnd": "14.0.2",
    "react-dnd-html5-backend": "14.0.0",
    "react-dom": "17.0.2",
    "react-dropzone": "11.3.4",
    "react-final-form": "6.5.3",
    "react-phone-input-2": "^2.14.0",
    "react-popover": "0.5.10",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "reactstrap": "8.9.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@testing-library/user-event": "13.2.1",
    "@types/jest": "27.0.0",
    "@types/lodash": "4.14.172",
    "@types/node": "16.6.0",
    "@types/react": "17.0.17",
    "@types/react-dom": "17.0.9",
    "@types/react-router-dom": "5.1.8",
    "@types/reactstrap": "8.7.2",
    "@typescript-eslint/eslint-plugin": "4.29.1",
    "@typescript-eslint/parser": "4.29.1",
    "eslint": "7.32.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.9.0",
    "eslint-plugin-import": "2.24.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.24.0",
    "husky": "7.0.1",
    "lint-staged": "11.1.2",
    "prettier": "2.3.2",
    "prettier-plugin-organize-imports": "2.3.3",
    "typescript": "4.3.5"
  },
  "scripts": {
    "build": "npm run create-env && react-scripts build",
    "coverage": "npm run test -- --coverage --watchAll=false",
    "create": "npm run create-env",
    "create-env": "echo \"SKIP_PREFLIGHT_CHECK=true\nREACT_APP_VERSION=${npm_package_version}\nBUILD_TIME=\"$(date +%Y-%m-%dT%H:%M:%S%z)\"\n\" > .env",
    "lint": "eslint src --ext js,jsx,ts,tsx --max-warnings=0",
    "serve-build": "npm run build && cd server && npm install && node ./index.js",
    "start": "npm run create && react-scripts start",
    "test": "react-scripts test",
    "test-all": "npm run create-env && npm run lint && npm run ts && npm run test -- --watchAll=true",
    "ts": "tsc --version && tsc"
  },
  "lint-staged": {
    "{src,test}/**/*.{js,jsx,json,scss,tsx,ts}": [
      "prettier --single-quote --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/core/index.ts",
      "!src/index.tsx",
      "!src/i18n.ts",
      "!src/error.ts",
      "!src/registerServiceWorker.ts"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "files": [
    "dist",
    "version.txt"
  ]
}
