{
  "name": "react-playground",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@42.nl/jarb-final-form": "2.1.4",
    "bootstrap": "5.2.1",
    "final-form": "4.20.7",
    "final-form-arrays": "3.0.2",
    "immutability-helper": "^3.1.1",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dom": "18.2.0",
    "react-final-form": "6.5.9",
    "react-final-form-arrays": "3.1.3",
    "react-router-dom": "5.3.0",
    "react-scripts": "5.0.1",
    "reactstrap": "9.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.0.3",
    "@types/lodash": "4.14.185",
    "@types/node": "18.7.21",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "eslint": "8.24.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.8",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "prettier-plugin-organize-imports": "3.1.1",
    "typescript": "4.8.3"
  },
  "scripts": {
    "better-audit": "node node_modules/better-npm-audit audit",
    "build": "npm run create-env && react-scripts build",
    "coverage": "npm run test -- --coverage --watchAll=false",
    "create": "npm run create-env",
    "create-env": "echo \"SKIP_PREFLIGHT_CHECK=true\nREACT_APP_VERSION=${npm_package_version}\nBUILD_TIME=\"$(date +%Y-%m-%dT%H:%M:%S%z)\"\n\" > .env",
    "lint": "eslint src --ext js,jsx,ts,tsx --max-warnings=0",
    "prepare": "husky install",
    "serve-build": "npm run build && cd server && npm install && node ./index.js",
    "start": "npm run create && react-scripts start",
    "test": "react-scripts test",
    "test-all": "npm run create-env && npm run lint && npm run ts && npm run test -- --watchAll=true",
    "ts": "tsc --version && tsc"
  },
  "lint-staged": {
    "{src,test}/**/*.{js,jsx,json,scss,tsx,ts}": [
      "prettier --single-quote --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/core/index.ts",
      "!src/index.tsx",
      "!src/i18n.ts",
      "!src/error.ts",
      "!src/registerServiceWorker.ts"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "files": [
    "dist",
    "version.txt"
  ]
}
